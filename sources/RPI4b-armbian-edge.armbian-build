#!/bin/sh

# script to build Armbian/Trixie image with latest kernel using compile.sh
# see https://github.com/armbian/build
set -x
./compile.sh \
  BOARD=rpi4b \
  BRANCH=edge \
  RELEASE=trixie \
  BUILD_MINIMAL=yes \
  KERNEL_CONFIGURE=no \
  INSTALL_HEADERS=yes \
  ENABLE_EXTENSIONS="cloud-init fs-btrfs-support fs-f2fs-support fs-xfs-support" \
  #
